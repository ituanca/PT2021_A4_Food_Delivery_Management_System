<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="640.0" prefWidth="1000.0" style="-fx-background-color: lightblue;" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentationLayer.AdministratorController">
   <children>
      <Label layoutX="420.0" layoutY="60.0" text="Administrator">
         <font>
            <Font name="Bodoni MT" size="26.0" />
         </font>
      </Label>
      <Button id="btnImport" fx:id="btnImport" layoutX="416.0" layoutY="162.0" mnemonicParsing="false" onAction="#importProducts" text="Import products">
         <font>
            <Font name="Bodoni MT" size="20.0" />
         </font>
      </Button>
      <Button id="btnAdd" fx:id="btnAdd" layoutX="433.0" layoutY="223.0" mnemonicParsing="false" onAction="#addProduct" text="Add product">
         <font>
            <Font name="Bodoni MT" size="20.0" />
         </font>
      </Button>
      <Button id="btnDelete" fx:id="btnDelete" layoutX="424.0" layoutY="282.0" mnemonicParsing="false" onAction="#deleteProduct" text="Delete product">
         <font>
            <Font name="Bodoni MT" size="20.0" />
         </font>
      </Button>
      <Button id="btnModify" fx:id="btnModify" layoutX="421.0" layoutY="341.0" mnemonicParsing="false" onAction="#modifyProduct" text="Modify product">
         <font>
            <Font name="Bodoni MT" size="20.0" />
         </font>
      </Button>
      <Button id="btnCreate" fx:id="btnCreate" layoutX="415.0" layoutY="400.0" mnemonicParsing="false" onAction="#createNewProduct" text="Create new menu">
      <font>
         <Font name="Bodoni MT" size="20.0" />
      </font>
      </Button>
      <Button id="btnGenerateReports" fx:id="btnGenerateReports" layoutX="417.0" layoutY="459.0" mnemonicParsing="false" onAction="#generateReports" text="Generate reports">
         <font>
            <Font name="Bodoni MT" size="20.0" />
         </font>
      </Button>

      <Button id="btnGoBack" fx:id="btnGoBack" layoutX="857.0" layoutY="569.0" mnemonicParsing="false" onAction="#goBack" text="Go back">
         <font>
            <Font name="Bodoni MT" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
