<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: lightblue;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentationLayer.AdministratorController">
   <children>
      <Label layoutX="313.0" layoutY="59.0" text="Administrator">
         <font>
            <Font name="Bodoni MT" size="26.0" />
         </font>
      </Label>
      <Button id="btnImport" fx:id="btnImport" layoutX="176.0" layoutY="154.0" mnemonicParsing="false" onAction="#importProducts" text="Import products">
         <font>
            <Font name="Bodoni MT" size="20.0" />
         </font>
      </Button>
      <Button id="btnAdd" fx:id="btnAdd" layoutX="176.0" layoutY="215.0" mnemonicParsing="false" onAction="#addProduct" text="Add product">
         <font>
            <Font name="Bodoni MT" size="20.0" />
         </font>
      </Button>
      <Button id="btnDelete" fx:id="btnDelete" layoutX="313.0" layoutY="215.0" mnemonicParsing="false" onAction="#deleteProduct" text="Delete product">
         <font>
            <Font name="Bodoni MT" size="20.0" />
         </font>
      </Button>
      <Button id="btnModify" fx:id="btnModify" layoutX="468.0" layoutY="215.0" mnemonicParsing="false" onAction="#modifyProduct" text="Modify product">
         <font>
            <Font name="Bodoni MT" size="20.0" />
         </font>
      </Button>
      <Button id="btnCreate" fx:id="btnCreate" layoutX="176.0" layoutY="274.0" mnemonicParsing="false" onAction="#createNewProduct" text="Create new product">
      <font>
         <Font name="Bodoni MT" size="20.0" />
      </font>
      </Button>
      <Button id="btnGenerateReports" fx:id="btnGenerateReports" layoutX="175.0" layoutY="335.0" mnemonicParsing="false" onAction="#generateReports" text="Generate reports">
         <font>
            <Font name="Bodoni MT" size="20.0" />
         </font>
      </Button>

      <Button id="btnGoBack" fx:id="btnGoBack" layoutX="669.0" layoutY="431.0" mnemonicParsing="false" onAction="#goBack" text="Go back">
         <font>
            <Font name="Bodoni MT" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
